export { show, showPack, print, println }

String = tag".String"

fn print(x) {
  show(x)
}

fn println() {
  print("\n")
}

fn println(x) {
  print(x)
  println()
}

fn showPack(x) {
  print("pack(")
  show(tag(x))
  i = 1
  while i <= nparts(x) {
    print(", ")
    show(part(x, i))
    i = i + 1
  }
  print(")")
}

fn replshow(x) {
  show(x)
  println()
}

fn replshow(_: Nil) {}
