bundle { String(obj: JSObject) }

fn tojs(s: String) { part(s, 1) }

fn concat(a: String, b: String) {
  String(call(a, "concat", b))
}

fn (a: String) == (b: String) {
  shortcutEquals(a, b) || equal(tojs(a), tojs(b))
}

fn string(x: Tag) {
  tagstring(x)
}

fn (a: Tag) == (b: Tag) {
  shortcutEquals(a, b)
}
