export { String, string, concat }

bundle String(obj: JSObject)

fn js(s: String) { part(s, 1) }

fn show(x: String) {
  print(js().JSON.stringify(x))
}

fn concat(a: String, b: String) {
  String(js(a).concat(b))
}

fn (a: String) == (b: String) {
  shortcutEquals(a, b) || (js(a) == js(b))
}

fn string(x: Tag) {
  tagstring(x)
}

fn (a: Tag) == (b: Tag) {
  shortcutEquals(a, b)
}
