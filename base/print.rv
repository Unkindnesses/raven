fn println(x) {
  print(x)
  print("\n")
}

fn print(x) {
  print("pack(")
  print(tag(x))
  i = 1
  while i <= nparts(x) {
    print(", ")
    print(part(x, i))
    i = i + 1
  }
  print(")")
}

fn print(s: String) { _print(s) }

fn print(x: Symbol) {
  print("`")
  print(string(x))
  print("`")
}

fn print(x: Int32) {
  print(string(x))
}

fn print(x: Int64) {
  print(string(x))
}

fn print(x: Nil) {
  print("nil")
}
