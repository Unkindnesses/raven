fn tag(x) { part(x, 0) }

fn Ptr(x::Int32) { data(`Ptr`, x) }
fn Ptr(x::Int64) { Ptr(Int32(x)) }
fn addr(x) { part(x, 1) }

fn pow(x, n) {
  r = 1
  while n > 0 {
    n = n - 1
    r = r * x
  }
  return r
}

fn relu(x) {
  if x > 0 {
    x
  } else {
    0
  }
}

pow(2, 3)
