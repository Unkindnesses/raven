Symbol  = `Symbol`
Tuple   = `Tuple`

fn tag(x) { part(x, 0) }

fn identity(x) { x }

fn (_ == _) { false }
fn isa(_, _) { false }

fn isa(data(`Tuple`, _...), `Tuple`) { true }
fn isa(data(`Symbol`, _), `Symbol`) { true }

fn (x |> f) { f(x) }

# fn tuple(xs...) { data(`Tuple`, xs...) }

fn condition(x) { panic("Non-boolean used as a condition") }
fn condition(x: Int32) { widen(x) }

data { Nil() }

nil = Nil()

fn (_: Nil) == (_: Nil) { true }

