fn String(obj: JSObject) { data(`String`, obj) }

fn isa(data(`String`, _), `String`) { true }

fn tojs(s: String) { part(s, 1) }

fn concat(a: String, b: String) {
  call(a, "concat", b)
}

fn (a: String) == (b: String) {
  equal(tojs(a), tojs(b))
}

fn string(x: Symbol) {
  symstring(x)
}

fn (a: Symbol) == (b: Symbol) {
  string(a) == string(b)
}
