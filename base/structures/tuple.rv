# data { Tuple(xs...) }

Tuple = `Tuple`

fn isa(data(`Tuple`, _...), `Tuple`) { true }

# fn tuple(xs...) { data(`Tuple`, xs...) }

fn print(x: Tuple) {
  print("[")
  i = 1
  while i <= nparts(x) {
    if (i != 1) {
      print(", ")
    }
    print(part(x, i))
    i = i + 1
  }
  print("]")
}

fn push(&xs, x) {
  xs = [xs..., x]
  return xs
}
