fn isPrime(n, primes) {
  for prime in primes {
    if rem(n, prime) == 0 {
      return false
    }
  }
  return true
}

fn sieve(n) {
  primes = []
  for i in range(2, n) {
    if isPrime(i, primes) {
      append(&primes, i)
    }
  }
  return primes
}

{
  primes = sieve(widen(100))
  println(part(primes, 1) == 2)
  println(part(primes, length(primes)) == 97)
}
