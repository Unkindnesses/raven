fn range(n) {
  xs = []
  for i in range(1, n) {
    append(&xs, i)
  }
  return xs
}

{
  xs = range(10)
  println(nparts(xs) == 10)
  println(part(xs, 1) == 1)
  println(part(xs, 10) == 10)
  println(part(xs, widen(5)) == 5)
}

fn rangeReverse(n) {
  xs = []
  i = 1
  while (i <= n) {
    xs = [i, xs...]
    i = i + 1
  }
  return xs
}

{
  xs = rangeReverse(10)
  println(nparts(xs) == 10)
  println(part(xs, 1) == 10)
  println(part(xs, 10) == 1)
  println(part(xs, widen(5)) == 6)
}

