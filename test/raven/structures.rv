fn range(n) {
  xs = []
  i = 1
  while (i <= n) {
    xs = [xs..., i]
    i = i + 1
  }
  return xs
}

zs = range(10)
println(nparts(zs) == 10)
println(part(zs, 1) == 1)
println(part(zs, 10) == 10)
println(part(zs, widen(5)) == 5)
